    function choiceClick(city){
       map.setCity(city,back);
       if(city=="北京市"){
        map.setZoomAndCenter(10, [116.205467, 39.907761]);
        $('#c_city_id').text(city);
       }else if (city=="上海市"){
         map.setZoomAndCenter(10, [121.72027588, 31.13642782]);
        var stext="<li class=\"map-measure\" map-on-click=\"measure\" onclick=\"choiceTop('迪斯尼')\"><span class=\"last measure\"> </span><i>迪斯尼</i></li>"
        $('#boxul')[0].innerHTML=stext;
        $('#c_city_id').text(city);
       }else if (city=="杭州市"){
         map.setZoomAndCenter(10, [120.29411316, 30.24779777]);
        $('#c_city_id').text(city);
        var stext="<li class=\"map-measure\" map-on-click=\"measure\" onclick=\"choiceTop('西湖')\"><span class=\"last measure\"> </span><i>西湖</i></li>"
        $('#boxul')[0].innerHTML=stext;
       }else if (city=="广州市"){
         map.setZoomAndCenter(10, [113.34869385, 23.09488913]);
        $('#c_city_id').text(city);
       }
    }
    
    function choiceTop(city){
       map.setCity(city,back);
       if(city=="迪斯尼"){
        map.setZoomAndCenter(16, [121.6641418032, 31.1433843458]);
       }else if (city=="上海市"){
         map.setZoomAndCenter(10, [121.72027588, 31.13642782]);
       }else if (city=="杭州市"){
         map.setZoomAndCenter(10, [120.29411316, 30.24779777]);
       }else if (city=="广州市"){
         map.setZoomAndCenter(10, [113.34869385, 23.09488913]);
       }else if (city=="西湖"){
         map.setZoomAndCenter(15, [120.1414890310, 30.2465755821]);
       }
    }
    function back(result){
        //alert(result);
    }
    function markerClick(e) {
        //infoWindow.setContent(e.target.content);
        //infoWindow.open(map, e.target.getPosition());
        $.ajax({
   		type:"post",
  		url:"/getSensor?id="+e.target.content,
   		success:function(data){
                     var sfull="";
                     var opennum="0";
                     var compressNum=0;
                     for(var i=0;i<data.sensor.length;i++){
                        se=data.sensor[i];
                        if (se.name==("垃圾是否满")){
                            sfull=se.tag;
                        }
                        if (se.name==("今天开门次数累计")){
                            opennum=se.value;
                        }
                        if (se.name==("今天70%启动压缩次数")){
                            compressNum+=parseInt(se.value);
                        }
                        if (se.name==("今天90%启动压缩次数")){
                            compressNum+=parseInt(se.value);
                        }
                     }
                     var info = [];
                     info.push("<div><div style=\"padding:0px 0px 0px 0px;\"><b>"+data.device.device_name+"</b></div>");
                     info.push("<div style=\"padding:0px 0px 0px 0px;line-height:18px;\">设备编号: "+data.device.device_sn+"");
                     info.push("垃圾桶状态: <div id=\"battery\"><div class=\"bottom\"></div><div class=\"middle\"><span class=\"glass\">80</span><div class=\"fill\"></div></div><div class=\"top-b\"></div></div>");
                     info.push("今天开门次数: "+"<div id=\"shiva\"><span class=\"count\">"+parseInt(opennum)+"</span></div>");
                     info.push("今天压缩次数: "+"<div id=\"shiva\"><span class=\"count\">"+compressNum+"</span></div>");
                     info.push("压缩比例: "+"<div class=\"green\"><div class=\"progress\"><div class=\"inner\"><div class=\"percent\">67%</div><div class=\"water\" style=\"top: 33%;\"></div><div class=\"glare\"></div></div></div></div>");
                     info.push("<a href='/devices/monitor?pid=<%=@project_id%>&device_id="+data.device.id+"'><font color='#3385ff'>详细信息</font></a>");
                     info.push("</div></div>");
                     infoWindow.setContent(info.join("<br/>"));
                     infoWindow.open(map, e.target.getPosition());
                     //alert("OK");
                }
        });
   }

</script>
