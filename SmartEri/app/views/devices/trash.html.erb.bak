<script>
var interval = 2000;
function progressAnimate(value) {
    var btn = document.getElementById('btn');
    btn.disabled = true;
    var drop = document.getElementById('drop');
    drop.style.webkitAnimationPlayState = 'running';
    var progress = document.getElementById('bucket');
    progress.value = value;
    value += 10;
    var t = setTimeout('progressAnimate(' + value + ')', interval);
    if (value === 100) {
        progress.value = 100;
        drop.style.webkitAnimationPlayState = 'paused';
        btn.disabled = false;
        clearTimeout(t);
    }
}
//# sourceURL=pen.js
</script>

